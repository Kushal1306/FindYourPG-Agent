{
    "rewrites": [
      {
        "source": "/assets/(.*)",
        "destination": "/ort/$1"
      },
      {
        "source": "/silero_vad.onnx",
        "destination": "/vad/silero_vad.onnx"
      },
      {
        "source": "/((?!vad|ort|assets).*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/vad/silero_vad.onnx",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/octet-stream"
          }
        ]
      },
      {
        "source": "/silero_vad.onnx",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/octet-stream"
          }
        ]
      },
      {
        "source": "/ort/(.*)\\.wasm",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/wasm"
          }
        ]
      },
      {
        "source": "/ort/(.*)\\.mjs",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      },
      {
        "source": "/assets/(.*)\\.wasm",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/wasm"
          }
        ]
      },
      {
        "source": "/assets/(.*)\\.mjs",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      },
      {
        "source": "/(.*)\\.js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ]
  }